<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.beetle-bsnes"
		name="SNES / Super Famicom (Mednafen bsnes)"
		version="0.9.26"
		provider-name="Libretro">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
		<import addon="game.controller.snes" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms>SNES</platforms>
		<extensions>smc|fig|bs|st|sfc</extensions>
		<supports_vfs>false</supports_vfs>
		<supports_standalone>false</supports_standalone>
		<supports_keyboard>false</supports_keyboard>
		<supports_mouse>false</supports_mouse>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">SNES / Super Famicom (Mednafen bsnes) v0.9.26</summary>
		<description>Beetle bSNES is the SNES module of Beetle, a fork of Mednafen. Mednafen&#39;s SNES module itself is a fork of bSNES v0.59.

Supported files: .smc, .fig, .bs, .st, .sfc

License: GPLv2</description>
		<license>GPLv2</license>
		<platform>@PLATFORM@</platform>
	</extension>
</addon>